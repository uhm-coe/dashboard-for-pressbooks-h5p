{
  "extends": [ "plugin:@wordpress/eslint-plugin/recommended" ],
  "rules": {
    // Disable conflicting whitespace rule.
    "prettier/prettier": "off",
    // Disable conflicting camelCase rule.
    "camelcase": "off",
    // Disable picky rule that seems to conflict with actual standard.
    "wordpress/array-function-spacing": "off",
    // Disable rule that conflicts with WordPress IIFE standard headers.
    "no-unused-expressions": "off",
    // Disable overly picky rule about chaining functions.
    "newline-per-chained-call": "off",
    // Disable yoda conditions.
    "yoda": "off"
  }
}
